            }
            .containerBox h1 {
                display: inline-block;
                font-size: 18px; /*or whatever you want*/
                color: #FFF;
                margin:0;
            }
            .containerBox p, .containerBox table{
                display:inline-block;
                color:#FFF;
            }
            .containerBox table{
                width:80%;
                display:table;
            }
            .containerBox table td{
                text-align: left;
            }
            .containerBox img {
                display: block;
                max-width: 100%;
                height: auto;
            }
            strong{
                font-weight: 700;
            }
            .card .card-image img {
                display: block;
                border-radius: 2px 2px 0 0;
                position:static;
                left: 0;
                right: 0;
                top: 0;
                bottom: 0;
                width: 100%;
            }
            .banner-images{
                position: relative;
                background: #F57558;
                opacity: 0.9;
                height: auto;
                width: auto;
                padding: 25px 17px 17px 17px;
            }
            .image-text {
                font-size: 14px;
                font-weight: 300;
                color: #666666;
                width: 100%;
                background: #FFFFFF;
                text-align: center;
                padding: 15px;
                margin-top: -5px;
                box-shadow: 0px 1px 1px #00000029;
            }
            .image-text-combo{
                bottom: 14%;
                font-size: 14px;
                font-weight: 300;
                color: black
            }
            .imageDiv {
                position: relative;
                background: #FFFFFF;
                padding: 5px 5px 0px 5px !important;
            }
            .corporate-product-card {
