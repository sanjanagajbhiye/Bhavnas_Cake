        body {
            font-size: 100%;
        }

        .card .card-content .card-title {
            margin-bottom: 0;
        }

        .text {
            font-size: 17px;
            color: #333333;
        }

        .button-style {
            border: 1px solid #D68563;
            font-size: 1.2vw;
