            color: #FFFFFF;
            border-radius: 5px;
            padding: 0.7vw 0 0.7vw;
            background-color: #DA4C64;
        }

        .card .card-content {
            padding: 18px;
        }

        .first-row > .row > .col.l2.m2 {
            display: inline-block;
            max-width: 14.28% !important;
            text-align: center !important;
            padding: 0;
            ;
        }

        .safe_portn .img_left {
            width: 34%;
            float: left;
        }

        .choose_gifts .select-wrapper input.select-dropdown {
            margin-bottom: 0;
            border-bottom: none;
        }

        .wide-range-text {
            width: 85%;
            margin-right: auto;
            margin-left: auto;
            left: 0;
            right: 0;
            background-color: #FFFFFF;
            z-index: 2;
            bottom: 18px;
            position: absolute;
            font-size: 14px;
            padding: 1px 10px;
            border-radius: 6px;
            color: #666666;
        }

        .new-arrive > .row > .col.s4 {
            padding: 0 0.3% 0;
        }

        .safe-delivery > .row > .col.s3 {
            padding: 0 0.2% 0;
        }

        .desk_banners {
            display: none;
            transition: opacity 1s ease;
            -webkit-transition: opacity 1s ease;
        }

            .desk_banners.slick-initialized {
                visibility: visible;
                opacity: 1;
            }

        .desk_banner img.lazyload {
            max-height: 372px;
        }

        .desk_banner picture {
            text-align: center;
            width: 100%;
            margin: 0 auto;
            float: left;
        }

        .relation_dropdwn {
            margin: 0;
            max-height: 100%;
            overflow-y: auto;
            height: 400px;
        }

        .choose_option {
            margin: 0 0 10px;
            text-align: left;
            padding: 0 0 20px;
            width: 100%;
        }

            .choose_option li {
                font-size: 14px;
                color: #333;
                line-height: 31px;
                padding: 2px 10px;
                width: 100%;
                display: block;
            }

                .choose_option li:first-child {
                    display: none;
                }

        .relation_dropdwn select {
            display: none;
        }

        .label_finder span.selected {
            color: #1BA23E !important;
        }

        .label_finder {
            position: relative;
        }

            .label_finder:after {
                content: "";
                width: 40px;
                height: 44px;
                top: 0px;
                right: -10px;
                pointer-events: none;
                background: #FFF url("//assets.winni.in/img/down-arrow-header.png") no-repeat 10px 12px;
                position: absolute;
            }

        .card_hover .zoom_img {
            border: none;
        }

        .card_hover:hover {
            box-shadow: 0px 1px 5px #00000029;
        }

        .card_hover {
            border-radius: 15px !important;
            box-shadow: 0px 1px 2px #f5f5f5;
            border: 1px solid #F5F5F5;
            padding: 0 !important;
            display: block;
            overflow: hidden !important;
            background-color: #F4F4F4 !important;
        }

        .zoom_img {
            display: block;
            max-width: 100%;
            overflow: hidden;
            border: 4px solid #fff;
        }

            .zoom_img img {
                transform-origin: 50% 65%;
                transition: transform 1s, filter 3s ease-in-out;
                width: 100%;
                display: block;
            }

        .card_hover:hover img {
            transform: scale(1.1);
        }

        .occasion-cat {
            font-size: 15px;
            color: #666666;
            display: block;
            opacity: 0.9;
        }

        .occasion-text {
            display: block;
            font-size: 24px;
            color: #333;
            line-height: 23px;
            background: #ffffff;
            z-index: 9;
        }

        .rel-cat {
